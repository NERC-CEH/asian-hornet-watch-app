{
  "version": "2.1.2",
  "scripts": {
    "start": "webpack serve --progress --port 8000",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync && npm run build:resources",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/*.png resources && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "deploy": "cp other/demo.html build/ && URL=$(vercel --prod -t $VERCEL_TOKEN --confirm ./build) && CLEAN_URL=$(echo $URL | sed -r \"s/https:\\/\\/(.*)$/\\1/g\") && DEMO_URL=$(echo $CLEAN_URL | sed -r \"s/(.*\\-)[a-z0-9]+\\-flumens(\\.vercel\\.app)$/\\1${BITRISE_BUILD_NUMBER}\\2/g\") && vercel alias set $CLEAN_URL $DEMO_URL -t $VERCEL_TOKEN && envman add --key DEMO_URL --value \"https://$DEMO_URL\"",
    "deploy:production": "bitrise ci",
    "clean": "rm -rf build resources",
    "test": "jest --silent --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch test",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .ts --ext .tsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.5",
    "@capacitor/app": "^5.0.3",
    "@capacitor/camera": "^5.0.4",
    "@capacitor/core": "^5.0.5",
    "@capacitor/device": "^5.0.4",
    "@capacitor/filesystem": "^5.0.4",
    "@capacitor/geolocation": "^5.0.4",
    "@capacitor/ios": "^5.0.5",
    "@capacitor/network": "^5.0.4",
    "@capacitor/splash-screen": "^5.0.4",
    "@capacitor/status-bar": "^5.0.4",
    "@flumens/ionic": "^1.26.1",
    "@ionic-native/core": "^5.36.0",
    "@ionic/core": "^7.0.12",
    "@ionic/react": "^7.0.12",
    "@ionic/react-router": "^7.0.12",
    "@sentry/browser": "^7.54.0",
    "@sentry/integrations": "^7.54.0",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "cordova-sqlite-storage": "^6.1.0",
    "formik": "^2.4.1",
    "i18next": "^22.5.1",
    "ionicons": "^7.1.2",
    "leaflet": "^1.9.4",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.17.1",
    "mapbox-gl": "2.4.1",
    "react-map-gl": "^7.0.25",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "swiper": "^9.4.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@capacitor/cli": "^5.0.4",
    "@flumens/eslint-config": "^2.0.1",
    "@flumens/fetch-onedrive-excel": "^0.3.0",
    "@flumens/prettier-config": "^0.2.3",
    "@flumens/webpack-config": "^5.2.0",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "^29.5.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "cordova-res": "^0.15.4",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "2.0.0",
    "regenerator-runtime": "^0.13.11",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4",
    "vercel": "^30.0.0",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  },
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "babel": {
    "presets": [
      [
        "@babel/react",
        {
          "runtime": "automatic"
        }
      ],
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "loose": true,
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ],
      [
        "@babel/typescript",
        {
          "allowDeclareFields": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@flumens$": "./src/common/flumens",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/proposal-private-property-in-object",
        {
          "loose": true
        }
      ],
      "@babel/transform-exponentiation-operator",
      "@babel/syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@flumens",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "react/function-component-definition": 0,
      "arrow-body-style": 0,
      "camelcase": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "prefer-arrow-callback": 0,
      "react/no-unused-class-component-methods": 0,
      "sort-class-members/sort-class-members": 0,
      "react/prop-types": 0,
      "react/jsx-no-useless-fragment": 0,
      "@typescript-eslint/no-empty-function": 0,
      "react/jsx-no-bind": 0,
      "class-methods-use-this": 0,
      "@typescript-eslint/no-var-requires": 0,
      "no-restricted-exports": 0,
      "default-param-last": 0,
      "@typescript-eslint/no-this-alias": 0,
      "@getify/proper-arrows/name": 0
    }
  },
  "prettier": "@flumens/prettier-config",
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/env",
              {
                "loose": true
              }
            ]
          ]
        }
      ]
    },
    "testRegex": "src.*((\\.|/*.)(spec))\\.(t|j)s?$",
    "moduleNameMapper": {
      "^.+.(css|style|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "swiper": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|@stencil|ionicons)"
    ]
  }
}
