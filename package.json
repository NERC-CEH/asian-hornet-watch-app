{
  "version": "2.0.2",
  "scripts": {
    "start": "webpack serve --progress --port 8000",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync && npm run build:resources",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/*.png resources && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "deploy": "cp other/demo.html build/ && URL=$(vercel --prod -t $VERCEL_TOKEN --confirm ./build) && CLEAN_URL=$(echo $URL | sed -r \"s/https:\\/\\/(.*)$/\\1/g\") && DEMO_URL=$(echo $CLEAN_URL | sed -r \"s/(.*\\-)[a-z0-9]+\\-flumens(\\.vercel\\.app)$/\\1${BITRISE_BUILD_NUMBER}\\2/g\") && vercel alias set $CLEAN_URL $DEMO_URL -t $VERCEL_TOKEN && envman add --key DEMO_URL --value \"https://$DEMO_URL\"",
    "deploy:production": "bitrise ci",
    "clean": "rm -rf build resources",
    "test": "jest --silent --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch test",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .ts --ext .tsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'"
  },
  "dependencies": {
    "@bit/flumens.apps.attr": "^2.2.9",
    "@bit/flumens.apps.attr-page": "^3.0.14",
    "@bit/flumens.apps.collapse": "^1.0.0",
    "@bit/flumens.apps.date": "^0.2.1",
    "@bit/flumens.apps.device": "^0.3.4",
    "@bit/flumens.apps.gallery": "^2.0.2",
    "@bit/flumens.apps.header": "^0.7.0",
    "@bit/flumens.apps.helpers.alert": "^2.0.0",
    "@bit/flumens.apps.helpers.image": "^0.0.2",
    "@bit/flumens.apps.helpers.loader": "^1.0.0",
    "@bit/flumens.apps.helpers.toast": "^1.0.0",
    "@bit/flumens.apps.info-background-message": "^1.0.1",
    "@bit/flumens.apps.info-message": "^1.2.1",
    "@bit/flumens.apps.input-with-validation": "^0.4.1",
    "@bit/flumens.apps.main": "^1.0.0",
    "@bit/flumens.apps.menu-attr-item": "^1.2.4",
    "@bit/flumens.apps.menu-attr-item-from-model": "^2.0.3",
    "@bit/flumens.apps.menu-attr-toggle": "^1.0.3",
    "@bit/flumens.apps.model-location-map": "^3.1.4",
    "@bit/flumens.apps.models.drupal-user-model": "1.0.3",
    "@bit/flumens.apps.models.init-stored-samples": "^0.1.0",
    "@bit/flumens.apps.models.media": "^0.2.8",
    "@bit/flumens.apps.models.model": "^2.2.2",
    "@bit/flumens.apps.models.occurrence": "^0.4.2",
    "@bit/flumens.apps.models.sample": "1.0.1",
    "@bit/flumens.apps.models.store": "^0.4.0",
    "@bit/flumens.apps.page": "^1.1.0",
    "@bit/flumens.apps.photo-picker": "3.0.4",
    "@bit/flumens.apps.route-with-models": "^1.0.1",
    "@bit/flumens.apps.section": "^0.4.1",
    "@bit/flumens.apps.utils.analytics": "^0.2.1",
    "@bit/flumens.apps.utils.location": "0.1.7",
    "@bit/flumens.apps.utils.validation": "^0.4.15",
    "@capacitor/android": "^3.4.3",
    "@capacitor/app": "^1.1.1",
    "@capacitor/camera": "^1.3.1",
    "@capacitor/core": "^3.4.3",
    "@capacitor/device": "^1.1.2",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/geolocation": "^1.3.1",
    "@capacitor/ios": "^3.4.3",
    "@capacitor/network": "^1.0.7",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@indicia-js/core": "^6.1.0",
    "@indicia-js/remote": "^6.1.4",
    "@ionic-native/core": "^5.36.0",
    "@ionic/core": "^6.1.3",
    "@ionic/react": "^6.1.3",
    "@ionic/react-router": "^6.1.3",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "cordova-sqlite-storage": "^6.0.0",
    "formik": "^2.2.9",
    "i18next": "^21.6.14",
    "ionicons": "^6.0.1",
    "leaflet": "^1.7.1",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-leaflet": "^3.2.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "swiper": "^8.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.8",
    "@capacitor/cli": "^3.4.3",
    "@flumens/eslint-config": "2.0.0",
    "@flumens/webpack-config": "^4.1.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cordova-res": "^0.15.4",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "mock-geolocation": "^1.0.11",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "regenerator-runtime": "^0.13.9",
    "typescript": "^4.6.3",
    "vercel": "^24.0.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.2"
  },
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/typescript",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@flumens": "./src/common/flumens",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/proposal-private-property-in-object",
        {
          "loose": true
        }
      ],
      "@babel/transform-exponentiation-operator",
      [
        "@babel/proposal-private-methods",
        {
          "loose": true
        }
      ],
      [
        "@babel/proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@flumens"
    ],
    "rules": {
      "react/function-component-definition": 0,
      "arrow-body-style": 0,
      "camelcase": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "prefer-arrow-callback": 0,
      "react/no-unused-class-component-methods": 0,
      "sort-class-members/sort-class-members": 0,
      "react/prop-types": 0,
      "react/jsx-no-useless-fragment": 0,
      "@typescript-eslint/no-empty-function": 0,
      "react/jsx-no-bind": 0,
      "class-methods-use-this": 0,
      "@typescript-eslint/no-var-requires": 0,
      "no-restricted-exports": 0,
      "default-param-last": 0,
      "@typescript-eslint/no-this-alias": 0,
      "@getify/proper-arrows/params": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.21"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/env"
          ]
        }
      ]
    },
    "testRegex": "((src/\\.|/*.)(spec))\\.js?$",
    "moduleNameMapper": {
      "^.+.(css|style|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "swiper": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|@bit|@stencil|ionicons)"
    ]
  }
}
